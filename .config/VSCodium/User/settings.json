{
	"[dockerfile]": {
		"editor.defaultFormatter": "ms-azuretools.vscode-docker"
	},
	"[go]": {
		"editor.defaultFormatter": "golang.go"
	},
	"[markdown]": {
		"editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
	},
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"codesnap.shutterAction": "copy",
	"diffEditor.codeLens": false,
	"editor.accessibilitySupport": "off",
	"editor.codeActionsOnSave": {
		"source.fixAll": "explicit",
		"source.organizeImports": "explicit"
	},
	"editor.cursorBlinking": "smooth",
	"editor.cursorWidth": 6,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.fontFamily": "JetBrains Mono",
	// "editor.fontFamily": "CaskaydiaCove Nerd Font",
	"editor.fontLigatures": true,
	"editor.fontSize": 14,
	"editor.fontWeight": "400",
	"editor.formatOnSave": true,
	"editor.guides.bracketPairs": "active",
	"editor.guides.bracketPairsHorizontal": true,
	"editor.inlayHints.fontSize": 12,
	"editor.inlineSuggest.enabled": true,
	"editor.lineHeight": 20,
	"editor.linkedEditing": true,
	"editor.maxTokenizationLineLength": 1000,
	"editor.minimap.enabled": true,
	"editor.minimap.renderCharacters": false,
	"editor.mouseWheelScrollSensitivity": 1.5,
	"editor.semanticHighlighting.enabled": false,
	"editor.smoothScrolling": true,
	"editor.stickyScroll.enabled": false,
	"editor.stickyTabStops": true,
	"editor.suggest.showWords": true,
	"editor.suggestSelection": "first",
	"editor.tabSize": 4,
	"editor.wordWrap": "on",
	"errorLens.fontSize": "12px",
	"explorer.compactFolders": false,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"files.associations": {
		"*.env*": "dotenv",
		".env*": "dotenv"
	},
	"files.eol": "\n",
	"files.insertFinalNewline": true,
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"gitlens.codeLens.enabled": false,
	"gitlens.currentLine.enabled": false,
	"gitlens.hovers.currentLine.enabled": false,
	"gitlens.hovers.currentLine.over": "line",
	"gitlens.launchpad.indicator.enabled": false,
	"gitlens.statusBar.enabled": false,
	"gitlens.telemetry.enabled": false,
	"go.inlayHints.parameterNames": true,
	"javascript.autoClosingTags": true,
	"javascript.inlayHints.parameterNames.enabled": "all",
	"javascript.inlayHints.parameterTypes.enabled": true,
	"javascript.preferences.renameMatchingJsxTags": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"markdown-preview-enhanced.previewTheme": "atom-dark.css",
	"markdown.preview.typographer": true,
	"prettier.bracketSameLine": false,
	"prettier.bracketSpacing": true,
	"prettier.printWidth": 95,
	"prettier.tabWidth": 2,
	"prettier.useTabs": true,
	"prettier.vueIndentScriptAndStyle": true,
	"search.exclude": {
		"**/node_modules": true,
		"**/dist": true,
		"**/build": true,
		"**/.git": true,
		"**/.DS_Store": true
	},
	"security.workspace.trust.untrustedFiles": "open",
	"telemetry.telemetryLevel": "off",
	"template-string-converter.autoRemoveTemplateString": true,
	"template-string-converter.validLanguages": [
		"svelte",
		"typescript",
		"javascript",
		"typescriptreact",
		"javascriptreact",
		"vue"
	],
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.cursorWidth": 5,
	"terminal.integrated.fontFamily": "CaskaydiaCove Nerd Font",
	"terminal.integrated.fontSize": 13,
	"terminal.integrated.gpuAcceleration": "on",
	"terminal.integrated.scrollback": 500,
	"terminal.integrated.smoothScrolling": true,
	"typescript.inlayHints.parameterNames.enabled": "all",
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.preferences.renameMatchingJsxTags": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"workbench.editor.wrapTabs": true,
	"workbench.list.smoothScrolling": true,
	"workbench.sideBar.location": "right",
	"workbench.startupEditor": "none",
	"workbench.tree.indent": 15,
	"window.menuBarVisibility": "toggle",
	"window.zoomLevel": 1,
	"workbench.iconTheme": "bearded-icons"
}
